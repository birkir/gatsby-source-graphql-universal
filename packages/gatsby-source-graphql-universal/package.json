{
  "name": "gatsby-source-graphql-universal",
  "description": "Gatsby plugin which adds a third-party GraphQL API to Gatsby GraphQL with client side execution",
  "version": "3.4.0",
  "author": "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
  "bugs": {
    "url": "https://github.com/birkir/gatsby-source-graphql-universal/issues"
  },
  "main": "index.js",
  "files": [
    "*.js",
    "index.d.ts",
    "third-party/*.js"
  ],
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "gatsby-source-graphql": "^2.5.2",
    "graphql-tag": "^2.10.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "^4.4.2",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.4",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.1.4"
  },
  "homepage": "https://github.com/birkir/gatsby-source-graphql-universal/#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "graphql",
    "universal"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^2.22.11",
    "graphql": "^14.6.0"
  },
  "repository": "https://github.com/birkir/gatsby-source-graphql-universal",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "clean": "rm -rf ./*.js"
  }
}
